    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="csrf-token" content={get_csrf_token()} />
    <.live_title suffix=" Â· Riva Ash">
      <%= assigns[:page_title] || "Riva Ash" %>
    </.live_title>

    <%# LiveDebugger browser features %>
